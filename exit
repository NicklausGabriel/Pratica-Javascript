[32m+[m[32mfunction calcularBhaskara(coef1, coef2, coef3) {[m
[32m+[m[32m    let delta = (coef2 * coef2) - (4 * coef1 * coef3);[m
[32m+[m
[32m+[m[32m    if (delta < 0) {[m
[32m+[m[32m        console.log("N√£o possui ra√≠zes reais.");[m
[32m+[m[32m    } else {[m
[32m+[m[32m        let raiz1 = (-coef2 + Math.sqrt(delta)) / (2 * coef1);[m
[32m+[m[32m        let reiz2 = (-coef2 - Math.sqrt(delta)) / (2 * coef1);[m
[32m+[m
[32m+[m[32m        document.getElementById("raiz1").innerHTML = `raiz1 = ${raiz1}`;[m
[32m+[m[32m        document.getElementById("raiz2").innerHTML = `raiz2 = ${reiz2}`;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m[32mlet coef1 = parseFloat(prompt("Digite o valor do primeiro coeficiente:"));[m
[32m+[m[32mlet coef2 = parseFloat(prompt("Digite o valor do segundo coeficiente:"));[m
[32m+[m[32mlet coef3 = parseFloat(prompt("Digite o valor do terceiro coeficiente:"));[m
[32m+[m
[32m+[m[32mcalcularBhaskara(coef1, coef2, coef3);[m
